<?xml version="1.0" encoding="utf-8"?>

<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
    <sitecore role:require="Standalone or ContentManagement">
        <settings>
            <!--  FACEBOOK - APPLICATION ID
                    Identifier for the SAML2-compatible service.
                    Typically the URL of the application for authentication.
                    e.g. http://sitecore.local/sitecore
            -->
            <setting name="Facebook.ApplicationId">
                <patch:attribute name="value">2357796427779470</patch:attribute>
            </setting>
            <!--  FACEBOOK - APPLICATION SECRET
                    The URL for the service to return to after successful authentication.
                    e.g. http://sitecore.local/sitecore
            -->
            <setting name="Facebook.ApplicationSecret">
                <patch:attribute name="value">caf3a1fd23753f910530f50de17c222c</patch:attribute>
            </setting>
        </settings>
    </sitecore>
</configuration>